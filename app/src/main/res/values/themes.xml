<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.LealAppsWorkout" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#48AF7F</item>
        <item name="colorPrimaryVariant">#222222</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#48AF7F</item>
        <item name="colorSecondaryVariant">#48AF7F</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#397C5D</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.LealAppsWorkout.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.LealAppsWorkout.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.LealAppsWorkout.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark" />
</resources>